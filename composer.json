{
    "name": "deadmansswitch/openapi",
    "version": "0.1.0",
    "description": "PHP API to work with OpenAPI schema. Allows to read and write OpenAPI Schema using PHP object representation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DeadMansSwitch\\OpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeadMansSwitch\\OpenApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Luke Kortunov",
            "email": "luke@kortunov.me"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "symfony/serializer": "^6.0|^7.0",
        "symfony/property-access": "^6.0|^7.0"

    },
    "require-dev": {
        "symfony/var-dumper": "^6.0|^7.0",
        "phpunit/phpunit": "^10"
    }
}
